{
  "expo": {
    "name": "TestCoding",
    "slug": "TestCoding",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_onboarding.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo_onboarding.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_onboarding.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.akhmaln.TestCoding"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2721cecd-f2a3-416c-a073-e2abeed98fa8"
      }
    }
  }
}
